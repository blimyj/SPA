procedure multipleIfWhile {
	start = 0;
	end = 100;
	if (start == 0) then {
		start = 0;
	} else {
		start = 0;
	}
	while (start < end) {
		start = start + 1;
	}
	start = start - end;
	middle = (start + end) / 2;
	if (middle > 0) then {
		print start;
	} else {
		print end;
	}
	if (middle < 0) then {
		print end;
	} else {
		print start;
	}
	while (middle < end) {
		end = end - 1;
		start = start + 1;
	}
}
	
	
