procedure firstElement {
    wind = earth + fire;
    print water;
    read earth;
    if ((!(earth == water)) && (wind <= fire)) then {
        fire = water - 4;
        read water;
        while (water < earth) {
            call secondElement;
        }
    } else {
        water = earth * water;
        print water;
        call secondElement;
        water = 4;
    }

}

procedure secondElement {
    water = earth * water;
    water = 4;
    read wind;
    read fire;
    print earth;
}